<%= "お部屋一覧" %>
<tbody>
<% @posts.each do |post| %>
    <tr>
    <td><%= post.id %></td>
    <td><%= post.room_name %></td>
    <td><%= post.introduction %></td>
    <td><%= post.price %></td>
    <td><%= post.address %></td>
    <td><%= image_tag @post.image.thumb.url %></td>
  </tr>
<% end %>
</tbody>

